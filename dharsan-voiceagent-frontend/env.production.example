# Frontend Production Environment Configuration
# Copy this file to .env.production and update with your actual values

# TURN Server Credentials (Replace with your actual credentials)
VITE_TURN_USERNAME=your_turn_username_here
VITE_TURN_CREDENTIAL=your_turn_credential_here

# Backend URLs (Update with your production URLs)
VITE_BACKEND_URL=https://your-backend-domain.com
VITE_WEBSOCKET_URL=wss://your-backend-domain.com/ws
VITE_WHIP_URL=https://your-backend-domain.com/whip

# Environment
NODE_ENV=production
VITE_ENVIRONMENT=production

# Analytics (Optional)
VITE_ANALYTICS_ID=your_analytics_id
VITE_SENTRY_DSN=your_sentry_dsn

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_METRICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Security
VITE_CONTENT_SECURITY_POLICY=default-src 'self' https: data: blob: 'unsafe-inline'
VITE_STRICT_TRANSPORT_SECURITY=max-age=31536000; includeSubDomains 